+  public static boolean checkForMatchingVisibilityTags(Cell cell,
+      List<Tag> visibilityTagsInDeleteCell) {
+    List<Tag> tags = getTagsFromResult(cell);
+    boolean pbMagicPrefix = true;
+    // From the first tag identify if it has PBUF. If yes then all the tags will
+    // be having PBUF
+    if (tags.size() > 0) {
+      pbMagicPrefix = VisibilityUtils.isPBMagicPrefixedTag(tags.get(0));
+    } else {
+      // Early out if there are no tags in the cell
+      return false;
+    }
+    // Check for the first byte to know
+    if (pbMagicPrefix) {
+      return checkForMatchingVisibilityTagsWithPBUF(cell.getTagsLength(),
+          visibilityTagsInDeleteCell, tags);
+    } else {
+      try {
+        return checkForMatchingVisibilityTagsWithOutPBUF(cell, visibilityTagsInDeleteCell);
+      } catch (Exception e) {
+        // Should not happen
